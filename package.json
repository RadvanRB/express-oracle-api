{
    "name": "express-oracle-api",
    "version": "1.0.0",
    "description": "REST API s Express.js, TypeORM, Zod a Oracle",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "concurrently \"npm run tsoa:watch\" \"nodemon --exec ts-node src/index.ts\"",
        "build": "npm run tsoa:build && tsc",
        "tsoa:build": "tsoa spec-and-routes",
        "tsoa:watch": "nodemon --exec \"tsoa spec-and-routes\" --ext ts --watch src/controllers"
    },
    "dependencies": {
        "@types/multer": "^1.4.12",
        "@types/swagger-ui-express": "^4.1.8",
        "axios": "^1.9.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "node-cache": "^5.1.2",
        "on-finished": "^2.4.1",
        "on-headers": "^1.0.2",
        "oracledb": "^6.2.0",
        "p-limit": "^4.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tsoa": "^6.6.0",
        "typeorm": "^0.3.17",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/compression": "^1.7.3",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.19",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/morgan": "^1.9.6",
        "@types/node": "^20.8.6",
        "@types/node-cache": "^4.1.3",
        "@types/oracledb": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "concurrently": "^9.1.2",
        "eslint": "^8.51.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
